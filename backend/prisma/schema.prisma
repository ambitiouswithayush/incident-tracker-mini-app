generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Incident {
  id        String   @id @default(uuid())
  title     String
  service   String
  severity  String
  status    String
  owner     String?
  summary   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
